{
    "inherit_from": ["spatial.json", "cloud.json"],
    "add_processes": [
        "concentrations_deriver"
    ],
    "total_time": 12000,
    "spatial_environment_config": {
        "multibody": {
            "bounds": [
                "!units[50 micrometer]",
                "!units[50 micrometer]"
            ]
        },
        "diffusion": {
            "molecules": ["GLC[p]", "nitrocefin", "cephaloridine", "tetracycline"],
            "n_bins": [10, 10],
            "bounds": [
                "!units[50 micrometer]",
                "!units[50 micrometer]"
            ],
            "depth": "!units[3000.0 micrometer]",
            "diffusion": "!units[1e-2 micrometer ** 2 / second]",
            "gradient": {
                "type": "uniform",
                "molecules": {
                    "GLC[p]": 1.0,
                    "nitrocefin": 0,
                    "cephaloridine": 0,
                    "tetracycline": 0
                }
            }
        },
        "field_timeline": {
            "bins": [10, 10]
        },
        "field_timeline_topology": {
            "global": ["global"],
            "fields": ["fields"]
        }
    },
    "topology": {
        "concentrations_deriver": {
            "counts": ["bulk"],
            "concentrations": ["periplasm", "concentrations"],
            "volume": ["periplasm", "global", "volume"]
        }
    },
    "process_configs": {
        "concentrations_deriver": {
            "variables": ["EG10040-MONOMER[p]", "TRANS-CPLX-201[m]"]
        }
    },
    "flow": {
        "concentrations_deriver": [["ecoli-shape"]]
    },
    "engine_process_reports": [
        ["bulk", "EG10040-MONOMER[p]"],
        ["bulk", "TRANS-CPLX-201[m]"],
        ["bulk", "CPLX0-7533[o]"],
        ["bulk", "CPLX0-7534[o]"],
        ["periplasm"],
        ["listeners", "mass"],
        ["boundary"]
    ]
}
