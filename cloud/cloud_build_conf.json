{
    "steps": [
    {
        "name": "gcr.io/cloud-builders/docker",
        "args": [
            "build",
            "--build-arg", "from=us-central1-docker.pkg.dev/${PROJECT_ID}/vecoli/${_WCM_RUNTIME}",
            "--build-arg", "git_hash=${_GIT_HASH}",
            "--build-arg", "git_branch=${_GIT_BRANCH}",
            "--build-arg", "timestamp=${_TIMESTAMP}",
            "-t", "us-central1-docker.pkg.dev/${PROJECT_ID}/vecoli/${_WCM_CODE}",
            "-f", "cloud/docker/wholecell/Dockerfile",
            "."
        ]
    }
    ],
    "images": [
        "us-central1-docker.pkg.dev/${PROJECT_ID}/vecoli/${_WCM_CODE}"
    ]
}
